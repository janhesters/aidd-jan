{
  "$schema": "https://turborepo.dev/schema.v2.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "check:lint": {},
    "check:types": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "tui"
}